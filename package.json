{
  "name": "ai-conversation-premium",
  "version": "2.0.0",
  "description": "Premium AI Conversation System - Enterprise-grade AI collaboration platform",
  "private": true,
  "author": {
    "name": "Piotr Tamulewicz",
    "email": "contact@petertam.pro",
    "url": "https://petertam.pro"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-conversation-premium"
  },
  "keywords": [
    "ai",
    "conversation",
    "nextjs",
    "typescript",
    "shadcn",
    "openrouter",
    "groq",
    "tts",
    "premium"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out dist",
    "analyze": "cross-env ANALYZE=true npm run build",
    "postinstall": "prisma generate || echo 'Prisma not configured'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
